{% extends 'base.html.twig' %}

{% block title %}New Fabricant{% endblock %}

{% block body %}
<main class="container">
<section class="row">
<div class="col-12">
<br>
    <h1>Ajouter un nouveau Fabricant</h1>
    <br>
    <br>

{{ form_start(form) }}
    {{ form_widget(form) }}
    <br>
    <button class="btn btn-success">{{ 'action.save'|trans }}</button>
    <a class="btn btn-primary me-md-2" href="{{ path('app_fabricant_index') }}">{{ 'action.back'|trans }}</a>
{{ form_end(form) }}

</div>
</section>
</main>
{% endblock %}
