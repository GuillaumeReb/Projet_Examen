{% extends 'base.html.twig' %}

{% block title %}Modifier marque{% endblock %}

{% block body %}
<main class="container">
<section class="row">
<div class="col-12">
<br>
    <h1>Modifier la Marque</h1>

    {{ form_start(form) }}
    {{ form_widget(form) }}
    <br>
    <button class="btn btn-success">{{ 'action.save'|trans }}</button>
